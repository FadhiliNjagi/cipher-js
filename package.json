{
  "name": "symmetric-cipher.js",
  "version": "0.1.2",
  "description": "A lightweight library meant for encrypting and decrypting cookies. Written for express-js",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "test": "ts-mocha test.ts",
    "lint": "eslint *.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FadhiliNjagi/cipher-js.git"
  },
  "keywords": [
    "symmetric",
    "encryption",
    "decryption"
  ],
  "author": "Fadhili Njagi (FadhiliNjagi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FadhiliNjagi/cipher-js/issues"
  },
  "homepage": "https://github.com/FadhiliNjagi/cipher-js#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "5.0.0",
    "chai": "^4.3.7",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "dependencies": {}
}
