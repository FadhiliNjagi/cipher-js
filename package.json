  },
  "devDependencies": {
    "@types/buffer-from": "^1.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
